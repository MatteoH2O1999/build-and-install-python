{
    "name": "build-and-install-python",
    "version": "0.0.0",
    "private": false,
    "description": "Build and Install action",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build --verbose",
        "format": "prettier --write **/*.ts",
        "format-check": "prettier --check **/*.ts",
        "lint": "eslint src/**/*.ts",
        "package": "ncc build ./lib/index.js --license licenses.txt && node ./.github/scripts/append_license",
        "test": "jest",
        "test-ci": "jest --reporters='@matteoh2o1999/github-actions-jest-reporter'",
        "complete-build": "npm run build && npm run package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MatteoH2O1999/build-and-install-python.git"
    },
    "keywords": [
        "actions",
        "python",
        "build",
        "setup"
    ],
    "author": "Matteo Dell'Acqua",
    "license": "AGPL-3.0-or-later",
    "dependencies": {
        "@actions/artifact": "^1.1.0",
        "@actions/cache": "^3.1.0-beta.3",
        "@actions/core": "^1.10.0",
        "@actions/exec": "^1.1.1",
        "@actions/github": "^5.1.1",
        "@actions/glob": "^0.3.0",
        "@actions/http-client": "^2.0.1",
        "@actions/io": "^1.1.2",
        "@actions/tool-cache": "^2.0.1",
        "semver": "^7.3.8"
    },
    "devDependencies": {
        "@matteoh2o1999/github-actions-jest-reporter": "^1.0.2",
        "@types/node": "^18.11.15",
        "@typescript-eslint/parser": "^5.46.1",
        "@vercel/ncc": "^0.36.0",
        "eslint": "^8.30.0",
        "eslint-plugin-github": "^4.6.0",
        "eslint-plugin-jest": "^27.1.7",
        "jest": "^29.3.1",
        "js-yaml": "^4.1.0",
        "prettier": "^2.8.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.4"
    }
}
